{
    "name": "jobsity/slim-challenge",
    "description": "PHP Challenge - Jobsity",
    "type": "project",
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.7",
        "doctrine/orm": "^3.0",
        "doctrine/dbal": "^4.0",
        "symfony/cache": "^7.1",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.9",
        "phpmailer/phpmailer": "^6.9",
        "php-amqplib/php-amqplib": "^3.7",
        "symfony/console": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "autoload": {
        "psr-0": {
            "": "app/"
        },
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
